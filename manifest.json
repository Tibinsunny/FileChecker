{
    "name": "FileChecker",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage","activeTab"
      ],
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action":{
        "default_popup":"index.html",
        "default_title":"File Checker"
    }
}